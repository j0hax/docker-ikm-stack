  "@jupyterlab/docmanager-extension:plugin": {
    "defaultViewers": {
      "markdown": "Markdown Preview"
    }
  },
  "@jupyterlab/translation-extension:plugin": {
    "locale": "de_DE"
  },
  "@jupyterlab/apputils-extension:themes": {
    "overrides": {
      "content-font-family": "Rotis Sans Serif Std",
